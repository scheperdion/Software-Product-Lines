				<feature name="Thunderstorm"/>
				<feature name="Earthquake"/>
			</and>
			<and name="Notifications">
				<feature name="SMS"/>
				<feature name="Bluetooth"/>
				<feature name="Push"/>
			</and>
			<alt name="Type">
				<and name="Client">
					<alt abstract="true" name="UI">
						<feature name="Alert_UI"/>
						<and name="Extended_UI">
							<feature name="Dark_theme"/>
						</and>
					</alt>
					<and abstract="true" name="Language">
						<feature name="English"/>
						<feature name="Dutch"/>
					</and>
				</and>
				<feature name="Server"/>
			</alt>
		</and>
	</struct>
	<constraints>
		<rule>
			<imp>
				<var>Events</var>
				<var>Notifications</var>
			</imp>
		</rule>
	</constraints>
</featureModel>
